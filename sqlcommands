CREATE TABLE "applications" (
	"application_id"	INTEGER NOT NULL UNIQUE,
	"application_status" TEXT NOT NULL,
	"application_progress" INTEGER NOT NULL,
	"team_id"	TEXT,
	"mymlh_uid"	INTEGER NOT NULL UNIQUE,
	"reimbusment"	TEXT NOT NULL,
	"travel_from"	TEXT,
	"visa"	INTEGER,
	"diet"	TEXT NOT NULL,
	"tshirt"	TEXT NOT NULL,
	"job_looking"	INTEGER NOT NULL,
	"job_preference"	TEXT,
	"cv_path"	TEXT UNIQUE,
	"skills"	TEXT NOT NULL,
	"excited_hk22"	TEXT,
	"first_hear_hk22"	TEXT,
	"first_hack_hk22"	INTEGER,
	"spirit_animal"	TEXT,
	"pizza"	TEXT,
	"site"	TEXT UNIQUE,
	"github"	TEXT UNIQUE,
	"devpost"	TEXT UNIQUE,
	"linkedin"	TEXT UNIQUE,
	PRIMARY KEY("application_id" AUTOINCREMENT)
);

CREATE TABLE "teams" (
	"team_id"	INTEGER NOT NULL UNIQUE,
	"team_code"	TEXT NOT NULL UNIQUE,
	"owner" INTEGER NOT NULL,
	PRIMARY KEY("team_id" AUTOINCREMENT)
);