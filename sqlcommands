CREATE TABLE "applications" (
	"application_id"	INTEGER UNIQUE,
	"application_status" TEXT,
	"application_progress" INTEGER,
	"team_id"	TEXT,
	"mymlh_uid"	INTEGER UNIQUE,
	"reimbusment"	TEXT,
	"travel_from"	TEXT,
	"visa"	TEXT,
	"diet"	TEXT,
	"tshirt"	TEXT,
	"job_looking"	TEXT,
	"job_preference"	TEXT,
	"cv_path"	TEXT UNIQUE,
	"skills"	TEXT,
	"excited_hk22"	TEXT,
	"hear_hk22"	TEXT,
	"first_hack_hk22"	TEXT,
	"spirit_animal"	TEXT,
	"pizza"	TEXT,
	"site"	TEXT UNIQUE,
	"github"	TEXT UNIQUE,
	"devpost"	TEXT UNIQUE,
	"linkedin"	TEXT UNIQUE,
	PRIMARY KEY("application_id" AUTOINCREMENT)
);

CREATE TABLE "teams" (
	"team_id"	INTEGER UNIQUE,
	"team_code"	TEXT UNIQUE,
	"owner" INTEGER,
	PRIMARY KEY("team_id" AUTOINCREMENT)
);

CREATE TABLE "votes" (
	"voter_uid"	INTEGER,
	"score"	INTEGER,
	"application_id"	INTEGER,
	PRIMARY KEY("application_id")
);